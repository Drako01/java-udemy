package com.udemy;

public class Variables {

	public static void main(String[] args) {
		int miVariableEntera = 10;
		System.out.println(miVariableEntera);
		
		miVariableEntera = 5;
		System.out.println(miVariableEntera);
		
		String miVariableCadena = "Hola";
		
		System.out.println(miVariableCadena);

	}

}